
$(function(){$(window).scroll(function(){if($(this).scrollTop()>100){$("#scrollToTop").fadeIn()}else{$("#scrollToTop").fadeOut()}});$("#scrollToTop").click(function(){$("html, body").animate({scrollTop:0},800);return false})});$(function(){$(".collapse").on("hide.bs.collapse",function(){var b=$(this).attr("id");if(b){var a=$("a[aria-controls='"+b+"'] .collapse-state");$(a).html("показать")}}).on("show.bs.collapse",function(){var b=$(this).attr("id");if(b){var a=$("a[aria-controls='"+b+"'] .collapse-state");$(a).html("скрыть")}})});$(function(){$("div.button-slider").hover(function(){$(this).stop().animate({marginLeft:"0"},500)},function(){$(this).stop().animate({marginLeft:"-170px"},500)});$("form#feedback-form, form#error-found-form").submit(function(){var e={context:this,url:$(this).attr("action"),type:"post",data:$(this).serialize(),dataType:"html",success:c,error:a,beforeSend:b,complete:d};$.ajax(e);return false});function b(){var e=$("button",this);$(e).prop("disabled",true);$('<span class="glyphicon glyphicon-refresh glyphicon-refresh-animate"></span>').css("padding","0 10px").prependTo(e)}function d(){var f=$("button",this);$(f).prop("disabled",false);$("span",f).remove();var e=$("a",this).attr("data-id");var g=Function("reload_"+e+"()");g()}function c(e){$(this).siblings(".modal-result").html(e)}function a(g,e,f){$(this).siblings(".modal-result").html(e+"<br/>"+f)}});$(function(){});$(function(){$("figure a:has(img)").each(function(){if(!$(this).attr("rel")){$(this).attr("rel","page-gallery")}}).fancybox({type:"image",nextEffect:"fade",prevEffect:"fade",margin:[15,15,40,15],padding:0,beforeLoad:function(){this.title=$(this.element).parents("figure").children("figcaption").html()},afterLoad:a,beforeClose:c,afterShow:b});function b(){var d=$(".fancybox-title-float-wrap");if(d){var e=-10-$(d).height();$(d).css("margin-bottom",e+"px")}}function a(){var e=$("#fbox-links");if(!e.length){e=$('<ul id="fbox-links">');for(var d=0;d<this.group.length;d++){$('<li data-index="'+d+'"><label></label></li>').click(function(){$.fancybox.jumpto($(this).data("index"))}).appendTo(e)}e.appendTo("body")}e.find("li").removeClass("active").eq(this.index).addClass("active")}function c(){$("#fbox-links").remove()}});$(function(){$("ol.russian").each(function(){$(this).children().each(function(c){var b=String.fromCharCode(1072+((c<9)?c:c+1));var a=$("<span>"+b+")</span>").css("margin-right","8px");$(this).prepend(a)})})});$(function(){var a="active";$(".panel-menu ul").mouseleave(function(){$('a[data-rp-markup*="active"]',this).addClass(a)});$(".panel-menu li a").hover(function(){var b=$(this).parents("ul");$("a.active",b).removeClass(a);$(this).addClass(a)},function(){$(this).removeClass(a)})});$(function(){var a=function(d){var c=1;d.prevAll("col").each(function(){if($(this).attr("span")!==undefined){c+=parseInt($(this).attr("span"))}else{++c}});return c};var b=function(h){var e=h.parents("table").get(0);var d=a(h);var g=1;if(h.attr("span")!==undefined){g=parseInt(h.attr("span"))}var c=$();for(var f=0;f<g;++f){c=c.add("tr td:nth-child("+(d+f)+")",e)}return c.not('[style*="text-align"]')};$("col").each(function(){var g=$(this);var k=g.attr("data-cell-class");var j=g.attr("data-cell-style");if(k||j){var d=b(g);if(k!==undefined){d.addClass(k)}if(j!==undefined){var c=j.split(";");var h={};var f;for(f=0;f<c.length;++f){var e=c[f].split(":");if(e[1]!==undefined){h[e[0].trim()]=e[1].trim()}}d.css(h)}}})});$(function(){$("a.tooltip-target").qtip({content:{text:function(a){var b=$(this).attr("href");return $(b).html()},title:{text:function(a){var b=$(this).attr("href");return $(b).attr("title")},button:"Close"}},show:{event:"click",solo:true},hide:{event:"click unfocus"},style:{classes:"ui-tooltip-shadow ui-tooltip-rounded qtip-bootstrap"}})});